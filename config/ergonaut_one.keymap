        };

        to0r24: ToBASER24 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_press &mt LSHIFT L &macro_pause_for_release &macro_release &mt LSHIFT L &macro_tap &to BASE>;
        };

        to0r25: ToBASER25 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_press &kpkp RG(SEMI) SEMI &macro_pause_for_release &macro_release &kpkp RG(SEMI) SEMI &macro_tap &to BASE>;
        };

        to0r26: ToBASER26 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_press &kpkp RG(SQT) SQT &macro_pause_for_release &macro_release &kpkp RG(SQT) SQT &macro_tap &to BASE>;
        };

